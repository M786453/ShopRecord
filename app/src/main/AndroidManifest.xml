<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shoprecord">

    <application
        android:allowBackup="true"
        android:icon="@drawable/record"
        android:label="@string/app_name"
        android:roundIcon="@drawable/record"
        android:supportsRtl="true"
        android:theme="@style/Theme.ShopRecord">
        <activity android:name=".StoreActivity"></activity>
        <activity android:name=".BillingActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>